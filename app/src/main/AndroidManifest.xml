<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="kz.sdu.kairatawer.ratemyhocam">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".PickEasy"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activities.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activities.LoginActivity" />
        <activity android:name=".activities.SignupActivity" />
        <activity android:name=".activities.InitialActivity" />
        <activity android:name=".activities.ViewAllActivity" />
        <activity android:name=".activities.TeacherProfileActivity" />
        <activity android:name=".activities.RateTeacherActivity" />
        <activity android:name=".activities.AfterRatingActivity"></activity>
    </application>

</manifest>